<Window x:Class="EnglishAimlessly2.View.demo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EnglishAimlessly2.View"
        xmlns:vm="clr-namespace:EnglishAimlessly2.ViewModel"
        mc:Ignorable="d"
        Title="demo" Height="450" Width="800">

    <Window.Resources>
        <vm:MainMenuVM x:Key="mmvm"/>
    </Window.Resources>
    
    <StackPanel Margin="10">
        <TextBlock Text="{Binding Source={StaticResource mmvm}, Path=LoggedUser.Name}"/>
        <TextBlock Text="{Binding Source={StaticResource mmvm}, Path=LoggedUser.Lastname}"/>
    </StackPanel>
</Window>
